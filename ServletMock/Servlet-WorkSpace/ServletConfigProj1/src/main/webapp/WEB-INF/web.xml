<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PageA</servlet-name>
  	<servlet-class>com.jsp.servletconfig.servlet.PageA</servlet-class>
  	<init-param>
  		<param-name>Doggo</param-name>
  		<param-value>5000</param-value>
  	</init-param>
  	<init-param>
  		<param-name>Retriever</param-name>
  		<param-value>7000</param-value>
  	</init-param>
  	<init-param>
  		<param-name>Chiuava</param-name>
  		<param-value>10000</param-value>
  	</init-param>
  	<init-param>
  		<param-name>Shephard</param-name>
  		<param-value>6000</param-value>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>PageB</servlet-name>
  	<servlet-class>com.jsp.servletconfig.servlet.PageB</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PageA</servlet-name>
  	<url-pattern>/PageA</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PageB</servlet-name>
  	<url-pattern>/PageB</url-pattern>
  </servlet-mapping>
</web-app>
